#include <LedControl.h>
LedControl display = LedControl(2,3,4,5);

void setup() {
  // put your setup code here, to run once:
  int dataPin=2;
  int clkPin=3;
  int csPin=4; 
  int NumDev=5;
  display.shutdown(0, false);
  display.setIntensity(0,3);
}

void loop() {
  // put your main code here, to run repeatedly:7
  display.setRow(0,0,0B00111100);
  display.setRow(0,1,0B01000010);
  display.setRow(0,2,0B10000001);
  display.setRow(0,3,0B10100101);
  display.setRow(0,4,0B10000001);
  display.setRow(0,5,0B10000001);
  display.setRow(0,6,0B01000010);
  display.setRow(0,7,0B00111100);

  delay(500);

  display.setRow(0,0,0B00111100);
  display.setRow(0,1,0B01000010);
  display.setRow(0,2,0B10000001);
  display.setRow(0,3,0B10100101);
  display.setRow(0,4,0B10000001);
  display.setRow(0,5,0B10111101);
  display.setRow(0,6,0B01011010);
  display.setRow(0,7,0B00111100);

  delay(500);

  display.setRow(0,0,0B00111100);
  display.setRow(0,1,0B01000010);
  display.setRow(0,2,0B10000001);
  display.setRow(0,3,0B10100101);
  display.setRow(0,4,0B10000001);
  display.setRow(0,5,0B10000001);
  display.setRow(0,6,0B01000010);
  display.setRow(0,7,0B00111100);

  delay(500);

  display.setRow(0,0,0B00111100);
  display.setRow(0,1,0B01000010);
  display.setRow(0,2,0B11000011);
  display.setRow(0,3,0B10100101);
  display.setRow(0,4,0B10000001);
  display.setRow(0,5,0B10011001);
  display.setRow(0,6,0B01111110);
  display.setRow(0,7,0B00111100);

  delay(500);

  display.setRow(0,0,0B00000000);
  display.setRow(0,1,0B00000000);
  display.setRow(0,2,0B00000000);
  display.setRow(0,3,0B00000000);
  display.setRow(0,4,0B00000000);
  display.setRow(0,5,0B00000000);
  display.setRow(0,6,0B00000000);
  display.setRow(0,7,0B00000000);

  delay(500);









}
